<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_container_point_test_injector"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cinza_claro_tecnomotor">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/nav_point_test_injector"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="true" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/cinza_claro_tecnomotor">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerview_list_point_test_injector"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"
            tools:listitem="@layout/point_test_injector_recyclerview2" />

        <ImageButton
            android:id="@+id/btn_cancel_point_test_injector"
            android:layout_width="278dp"
            android:layout_height="126dp"
            android:background="@color/cinza_claro_tecnomotor"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toTopOf="@+id/btn_pause_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/recyclerview_list_point_test_injector"
            app:srcCompat="@drawable/svg_button_medium_selector"
            tools:ignore="MissingConstraints,DuplicateSpeakableTextCheck,ImageContrastCheck" />

        <ImageView
            android:id="@+id/img_cancel_point_test_injector"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="40dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="@+id/btn_cancel_point_test_injector"
            app:layout_constraintStart_toStartOf="@+id/btn_cancel_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/btn_cancel_point_test_injector"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@drawable/ic_cancelar" />

        <TextView
            android:id="@+id/txt_cancel_point_test_injector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txt_cancel"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_cancel_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_cancel_point_test_injector"
            app:layout_constraintHorizontal_bias="0.474"
            app:layout_constraintStart_toEndOf="@+id/img_cancel_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/img_cancel_point_test_injector" />

        <ImageButton
            android:id="@+id/btn_pause_point_test_injector"
            android:layout_width="278dp"
            android:layout_height="126dp"
            android:background="@color/cinza_claro_tecnomotor"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toTopOf="@+id/btn_continue_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_continue_point_test_injector"
            app:srcCompat="@drawable/svg_button_medium_selector"
            tools:ignore="MissingConstraints,ImageContrastCheck" />

        <ImageView
            android:id="@+id/img_pause_point_test_injector"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="40dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="@+id/btn_pause_point_test_injector"
            app:layout_constraintStart_toStartOf="@+id/btn_pause_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/btn_pause_point_test_injector"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@drawable/ic_pause" />

        <TextView
            android:id="@+id/txt_pause_point_test_injector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txt_pause"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_pause_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_pause_point_test_injector"
            app:layout_constraintHorizontal_bias="0.474"
            app:layout_constraintStart_toEndOf="@+id/img_pause_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/img_pause_point_test_injector" />


        <ImageButton
            android:id="@+id/btn_continue_point_test_injector"
            android:layout_width="278dp"
            android:layout_height="126dp"
            android:background="@color/cinza_claro_tecnomotor"
            android:contentDescription="@string/todo"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@+id/btn_skip_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_skip_point_test_injector"
            app:srcCompat="@drawable/svg_button_medium_selector"
            tools:ignore="MissingConstraints,ImageContrastCheck" />

        <ImageView
            android:id="@+id/img_continue_point_test_injector"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="40dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="@+id/btn_continue_point_test_injector"
            app:layout_constraintStart_toStartOf="@+id/btn_continue_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/btn_continue_point_test_injector"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@drawable/ic_repeat" />

        <TextView
            android:id="@+id/txt_continue_point_test_injector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txt_repeat"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_continue_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_continue_point_test_injector"
            app:layout_constraintHorizontal_bias="0.474"
            app:layout_constraintStart_toEndOf="@+id/img_continue_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/img_continue_point_test_injector" />


        <ImageButton
            android:id="@+id/btn_skip_point_test_injector"
            android:layout_width="278dp"
            android:layout_height="126dp"
            android:background="@color/cinza_claro_tecnomotor"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toTopOf="@+id/btn_save_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_save_point_test_injector"
            app:srcCompat="@drawable/svg_button_medium_selector"
            tools:ignore="MissingConstraints,ImageContrastCheck" />

        <ImageView
            android:id="@+id/img_skip_point_test_injector"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="40dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="@+id/btn_skip_point_test_injector"
            app:layout_constraintStart_toStartOf="@+id/btn_skip_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/btn_skip_point_test_injector"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@drawable/ic_skip02" />

        <TextView
            android:id="@+id/txt_skip_point_test_injector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txt_skip"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_skip_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_skip_point_test_injector"
            app:layout_constraintHorizontal_bias="0.474"
            app:layout_constraintStart_toEndOf="@+id/img_skip_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/img_skip_point_test_injector" />


        <ImageButton
            android:id="@+id/btn_save_point_test_injector"
            android:layout_width="278dp"
            android:layout_height="126dp"
            android:background="@color/cinza_claro_tecnomotor"
            android:contentDescription="@string/todo"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@+id/btn_finish_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_finish_point_test_injector"
            app:srcCompat="@drawable/svg_button_medium_selector"
            tools:ignore="ImageContrastCheck" />

        <ImageView
            android:id="@+id/img_save_point_test_injector"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="40dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="@+id/btn_save_point_test_injector"
            app:layout_constraintStart_toStartOf="@+id/btn_save_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/btn_save_point_test_injector"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@drawable/ic_gravar" />

        <TextView
            android:id="@+id/txt_save_point_test_injector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txt_gravar"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_save_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_save_point_test_injector"
            app:layout_constraintHorizontal_bias="0.474"
            app:layout_constraintStart_toEndOf="@+id/img_save_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/img_save_point_test_injector"
            app:layout_constraintVertical_bias="0.547" />


        <ImageButton
            android:id="@+id/btn_finish_point_test_injector"
            android:layout_width="278dp"
            android:layout_height="126dp"
            android:background="@color/cinza_claro_tecnomotor"
            android:contentDescription="@string/todo"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/svg_button_medium_selector"
            tools:ignore="ImageContrastCheck" />

        <ImageView
            android:id="@+id/img_finish_point_test_injector"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="40dp"
            android:contentDescription="@string/todo"
            app:layout_constraintBottom_toBottomOf="@+id/btn_finish_point_test_injector"
            app:layout_constraintStart_toStartOf="@+id/btn_finish_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/btn_finish_point_test_injector"
            app:layout_constraintVertical_bias="0.40"
            app:srcCompat="@drawable/ic_terminar" />

        <TextView
            android:id="@+id/txt_finish_point_test_injector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txt_finish"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/img_finish_point_test_injector"
            app:layout_constraintEnd_toEndOf="@+id/btn_finish_point_test_injector"
            app:layout_constraintHorizontal_bias="0.474"
            app:layout_constraintStart_toEndOf="@+id/img_finish_point_test_injector"
            app:layout_constraintTop_toTopOf="@+id/img_finish_point_test_injector"
            app:layout_constraintVertical_bias="0.547" />

        <TextView
            android:id="@+id/txt_time_title_test_injector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="20dp"
            android:text="@string/tempo_teste"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/txt_time_test_injector"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="@string/time"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/txt_time_title_test_injector"
            app:layout_constraintStart_toEndOf="@+id/txt_time_title_test_injector" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>