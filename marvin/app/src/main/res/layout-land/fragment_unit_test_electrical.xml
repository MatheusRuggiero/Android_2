<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.unit_test.pump.UnitTestElectricalPumpFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/btn_HW_electric_teste_drv1"
            android:layout_width="150dp"
            android:layout_height="70dp"
            android:layout_marginStart="50dp"
            android:layout_marginTop="30dp"
            android:text="@string/drv_1"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btn_HW_electric_teste_drv2"
            android:layout_width="150dp"
            android:layout_height="70dp"
            android:layout_marginTop="30dp"
            android:text="@string/drv_2"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/btn_HW_electric_teste_drv1"
            app:layout_constraintTop_toBottomOf="@+id/btn_HW_electric_teste_drv1" />

        <Button
            android:id="@+id/btn_HW_electric_teste_ext1"
            android:layout_width="150dp"
            android:layout_height="70dp"
            android:layout_marginTop="30dp"
            android:text="@string/ext_1"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/btn_HW_electric_teste_drv2"
            app:layout_constraintTop_toBottomOf="@+id/btn_HW_electric_teste_drv2" />

        <Button
            android:id="@+id/btn_HW_electric_teste_ext2"
            android:layout_width="150dp"
            android:layout_height="70dp"
            android:layout_marginTop="30dp"
            android:text="@string/ext_2"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/btn_HW_electric_teste_ext1"
            app:layout_constraintTop_toBottomOf="@+id/btn_HW_electric_teste_ext1" />

        <TextView
            android:id="@+id/txt_HW_electrical_test_resistencia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txt_pump_test_resistencia"
            android:textColor="@color/pantone_black"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/btn_HW_electric_teste_drv2"
            app:layout_constraintEnd_toStartOf="@id/txt_HW_electrical_test_condition"
            app:layout_constraintTop_toTopOf="@+id/btn_HW_electric_teste_drv2" />

        <TextView
            android:id="@+id/txt_value_HW_electrical_test_resistencia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:textColor="@color/dark_gray"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/txt_HW_electrical_test_resistencia"
            app:layout_constraintTop_toBottomOf="@+id/txt_HW_electrical_test_resistencia"
            tools:text="Value Test" />

        <TextView
            android:id="@+id/txt_HW_electrical_test_condition"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/condicao"
            android:textColor="@color/pantone_black"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/txt_HW_electrical_test_resistencia"
            app:layout_constraintEnd_toStartOf="@id/txt_HW_electrical_test_status"
            app:layout_constraintTop_toTopOf="@+id/txt_HW_electrical_test_resistencia" />

        <TextView
            android:id="@+id/txt_value_HW_electrical_test_condition"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/dark_gray"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/txt_HW_electrical_test_condition"
            app:layout_constraintTop_toTopOf="@+id/txt_value_HW_electrical_test_resistencia"
            tools:text="Value Test" />

        <TextView
            android:id="@+id/txt_HW_electrical_test_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="@string/estado"
            android:textColor="@color/pantone_black"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/txt_HW_electrical_test_condition"
            app:layout_constraintTop_toTopOf="@+id/txt_HW_electrical_test_condition" />

        <TextView
            android:id="@+id/txt_value_HW_electrical_test_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/dark_gray"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/txt_HW_electrical_test_status"
            app:layout_constraintTop_toTopOf="@+id/txt_value_HW_electrical_test_resistencia"
            tools:text="Value Test" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>